{
	"masterPicture": "client/assets/images/master_favicon.svg",
	"iconsPath": "../static/favicons/",
	"design": {
		"ios": {
			"pictureAspect": "backgroundAndMargin",
			"backgroundColor": "#ffffff",
			"margin": "42%",
			"assets": {
				"ios6AndPriorIcons": false,
				"ios7AndLaterIcons": false,
				"precomposedIcons": false,
				"declareOnlyDefaultIcon": true
			},
			"appName": "Rulebook.io"
		},
		"desktopBrowser": {},
		"windows": {
			"pictureAspect": "noChange",
			"backgroundColor": "#b91d47",
			"onConflict": "override",
			"assets": {
				"windows80Ie10Tile": false,
				"windows10Ie11EdgeTiles": {
					"small": false,
					"medium": true,
					"big": false,
					"rectangle": false
				}
			},
			"appName": "Rulebook.io"
		},
		"androidChrome": {
			"pictureAspect": "backgroundAndMargin",
			"margin": "25%",
			"backgroundColor": "#ffffff",
			"themeColor": "#ffffff",
			"manifest": {
				"name": "Rulebook.io",
				"display": "standalone",
				"orientation": "notSet",
				"onConflict": "override",
				"declared": true
			},
			"assets": {
				"legacyIcon": false,
				"lowResolutionIcons": false
			}
		},
		"safariPinnedTab": {
			"pictureAspect": "blackAndWhite",
			"threshold": 42.03125,
			"themeColor": "#ff4d88"
		}
	},
	"settings": {
		"compression": 1,
		"scalingAlgorithm": "Mitchell",
		"errorOnImageTooSmall": false
	}
}
